{% set duckwaves = [
{"name": "Machine Structures", "url": url_for("ms.index")},
{"name": "Operating Systems", "url": url_for("os.index")},
{"name": "Data Structures and Algorithms", "url": ""},
{"name": "Language and Compilers", "url": ""},
{"name": "Computer Networking", "url": url_for("cn.index")},
{"name": "Fundamentals of Programming", "url": url_for("pg.index")}
]%}

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <!-- Open props -->
    <link rel="stylesheet" href="https://unpkg.com/open-props" />
    <!-- Bootstrap -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">
    <!-- Bootstrap icons -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.4/font/bootstrap-icons.css">
    <style>
        .pyduck-hero {
            font-weight: var(--font-weight-9);
            background: var(--gradient-2);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
        }

        .pyduck-title__duckwaves {
            font-weight: var(--font-weight-8);
            background: var(--gradient-3);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
        }

        .pyduck-title__duckwave_os {
            font-weight: var(--font-weight-8);
            background: var(--gradient-7);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
        }

        .pyduck-title__duckwave_ms,
        .pyduck-title__duckwave_pg {
            font-weight: var(--font-weight-8);
            background: var(--gradient-10);
            -webkit-background-clip: text;
            -webkit-text-fill-color: transparent;
        }

        .pyduck-para {
            max-width: 55ch;
            margin-inline: auto;
        }

        /*
        bootstrap dropdown caret off
        */
        .no-caret::after {
            display: none;
        }

        /* 
        stick footer 
        https://css-tricks.com/couple-takes-sticky-footer/#aa-there-is-flexbox
        */
        html,
        body {
            height: 100%;
        }

        body {
            display: flex;
            flex-direction: column;
        }

        .content {
            flex: 1 0 auto;
        }

        .footer {
            flex-shrink: 0;
        }

        [data-csduck-duckwave]:hover,
        [data-csduck-duckwave]:focus {
            animation: var(--animation-pulse);
        }
    </style>
    {% block css %}
    {% endblock css %}
</head>

<body>
    <div class="content">
        {% include "includes/header.html.jinja" %}
        {% block main %}
        <!-- [start] Hero -->
        {% include "includes/hero__home.html.jinja" %}
        <!-- Hero [end] -->
        <section class="container">
            <h1 class="fw-3 pyduck-title__duckwaves text-center">
                <i class="bi bi-tsunami"></i> Duckwaves
            </h1>
            <div class="row pt-3 row-cols-1 px-1 px-lg-5 row-cols-lg-3">
                {% for duckwave in duckwaves %}
                <div class="col p-1">
                    <div class="p-3 border rounded-2 position-relative" data-csduck-duckwave>
                        <a class="link link-body-emphasis
                            link-underline-secondary
                            link-underline-opacity-0 
                            link-offset-3-hover
                            link-underline-opacity-100-hover 
                            stretched-link" href="{{ duckwave.url }}">{{ duckwave.name }}</a>
                    </div>
                </div>
                {% endfor %}
            </div>
        </section>
        {% endblock main %}
    </div>
    <div class="container footer">
        <footer class="d-flex flex-wrap justify-content-between align-items-center py-3 my-4 border-top">
            <div class="col-md-4 d-flex align-items-center">
                <a class="d-flex align-items-center link link-secondary" href="#">
                    <div class="pt-1"><i class="bi bi-layers-half"></i></div>
                    <div class="ps-1 fw-bold"><span>csduck</span></div>
                </a>
                <span class="mb-3 mb-md-0 text-body-secondary">&copy; 2023 Company, Inc</span>
            </div>
        </footer>
    </div>
    <!-- Bootstrap -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe"
        crossorigin="anonymous"></script>
    <!-- htmx -->
    <script src="https://unpkg.com/htmx.org@1.9.0"
        integrity="sha384-aOxz9UdWG0yBiyrTwPeMibmaoq07/d3a96GCbb9x60f3mOt5zwkjdbcHFnKH8qls"
        crossorigin="anonymous"></script>
    <!-- Alpine.js -->
    <script defer src="https://cdn.jsdelivr.net/npm/alpinejs@3.12.0/dist/cdn.min.js"></script>

    {% block javascript %}
    {% endblock javascript %}
</body>

</html>